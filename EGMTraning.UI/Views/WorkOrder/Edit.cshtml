@model WorkOrderDto

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form asp-action="Edit">

    @Html.HiddenFor(x=>x.Id)
    <div class="form-group">
        <label class="col-sm-3 control-label no-adding-right">İş Emri Durumu</label>
        <div class="col-sm-9">
            <input asp-for="WorkOrderStatus" type="text"/>
        </div>
    </div>
    <br />
    <div class="form-group">
        <label class="col-sm-3 control-label no-adding-right">İş Emri Numarası</label>
        <div class="col-sm-9">
            <input asp-for="WorkOrderNumber" type="text" />
        </div>
    </div>
    <br />
    <div class="form-group">
        <label class="col-sm-3 control-label no-adding-right">İş Emri Açıklama</label>
        <div class="col-sm-9">
            <input asp-for=" WorkOrderDescription" type="text" />
        </div>
    </div>
    <br />
    <div class="clearfix form-actions">
        <div class="col-md-offset-3 col-md-9">
            <input type="submit" value="Güncelle" class="btn btn-info col-xs-3"/>
        </div>
    </div>

</form>

